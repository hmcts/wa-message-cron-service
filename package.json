  "scripts": {
    "start": "NODE_ENV=production ./node_modules/.bin/ts-node -r tsconfig-paths/register src/main/server.ts",
    "start:dev": "NODE_ENV=dev nodemon --config nodemon.json",
    "start:conditional": "bash -c 'if [ \"$NODE_ENV\" = \"dev\" ]; then yarn start:dev; else yarn start; fi'",
    "lint": "ESLINT_USE_FLAT_CONFIG=true eslint . --fix",
    "build": "yarn webpack --config webpack.config.js",
    "build:prod": "NODE_ENV=production yarn webpack --mode production --config webpack.config.js",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:routes": "jest -c jest.routes.config.js",
    "sonar-scan": "sonar-scanner",
    "test:a11y": "echo 'Not needed' && exit 0",
    "test:smoke": "yarn install && jest --config=jest.smoke.config.js && exit 0",
    "test:functional": "echo 'TODO add functional tests' && exit 1",
    "cichecks": "yarn && yarn build && yarn lint && yarn test && yarn test:routes",
    "audit": "yarn npm audit"
  },
